<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='portfolio'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            
  <inherits name='com.google.gwt.user.theme.clean.Clean'/>-->
  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->
	<inherits name='com.google.gwt.i18n.I18N'/>
	<extend-property name='locale' values='en_US,en_GB,de'/>
	<set-property-fallback name='locale' value='en_US'/>
	
	<inherits name="com.google.gwt.logging.Logging"/>
	<set-property name="gwt.logging.logLevel" value="INFO"/>
	<set-property name="gwt.logging.enabled" value="TRUE"/> # To disable logging

	<set-property name="gwt.logging.consoleHandler" value="ENABLED"/>  
	<set-property name="gwt.logging.developmentModeHandler" value="ENABLED" />
	<set-property name="gwt.logging.popupHandler" value="DISABLED" />
	<set-property name="gwt.logging.systemHandler" value="DISABLED" />
	<set-property name="gwt.logging.firebugHandler" value="ENABLED" />
	<set-property name="gwt.logging.simpleRemoteHandler" value="DISABLED" />

	<inherits name="com.smartgwt.SmartGwtNoTheme"/>
	<!--  <inherits name="com.smartclient.theme.graphite.Graphite"/>-->
	<inherits name="tenx.gwt.theme.newlook.NewLook"/>
	<inherits name="tenx.gwt.common.Common"/>
	
	<inherits name="com.smartgwt.extensions.SmartGWTExtension"/>
	
	 <inherits name='com.google.gwt.user.theme.clean.Clean'/>

  <!-- Specify the app entry point class.                         -->
  <entry-point class='tenx.gwt.portfolio.client.PortfolioMain'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
